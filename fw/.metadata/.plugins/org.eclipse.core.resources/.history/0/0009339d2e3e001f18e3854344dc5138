i2cM_t I2C_master_obj;
static uint16_t slaveAddr = (0x2<<1);

void I2C_master_init(I2C_HandleTypeDef *hi2c)
{
  I2C_master_obj.i2c_handler = hi2c;
  I2C_master_obj.slaveAddr = ;
  I2C_master_obj.txcount = 0;
  memset(&(I2C_slave_obj.data), 0, DATA_LENGTH);
}
void I2C_master_read(uint8_t *buffer, uint16_t Size, uint32_t Timeout)
{
  HAL_I2C_Master_Receive(hi2c, slaveAddr, buffer, DATA_LENGTH, 500);

}
