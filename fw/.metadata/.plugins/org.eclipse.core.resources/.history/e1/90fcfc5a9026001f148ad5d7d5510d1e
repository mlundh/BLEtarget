#ifndef SENSOR_H_
#define SENSOR_H_
#include "main.h"

typedef struct
{
  TIM_TypeDef *speedTimer; // 64MHz timer used to measure speed of pellet. Also used for timeout to report the hit.
  uint8_t buffer[9] = {0};// position of the hit. 0 Center, 1-7 channel, 8 back.
}sensor_t;


int Sensor_getPos();
float Sensor_getSpeed();
unsigned int Sensor_newValue();

#endif /* SENSOR_H_ */
